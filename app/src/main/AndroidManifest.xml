        </activity>
        <activity android:name=".CurrentPlayingActivity" />
        <activity android:name=".AllTrackActivity" />
        <activity android:name=".WorkoutTracksActivity" />
        <activity android:name=".MoodyTracksActivity" />
        <activity android:name=".StudyTracksActivity" />
        <activity android:name=".SummerTracksActivity" />
        <activity android:name=".LullabyTracksActivity" />
        <activity android:name=".HiphopTracksActivity" />
        <activity android:name=".PopTracksActivity" />
        <activity android:name=".RockTracksActivity" />
        <activity android:name=".MovieTracksActivity" />
        <activity android:name=".DanceTracksActivity" />
        <activity android:name=".PlaylistActivity" />
        <activity android:name=".Custom1TrackActivity" />
        <activity android:name=".Custom2TrackActivity" />
    </application>

</manifest>